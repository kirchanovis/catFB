{
  "name": "cat-test",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": "6.1.0",
    "node": "8.11.3"
  },
  "dependencies": {
    "react-scripts": "^1.1.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "node-sass": "^4.9.3",
    "onchange": "^4.1.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "lodash": "^4.17.11"
  },
  "scripts": {
    "scss": "node-sass --output-style compressed -o src/assets/css src/assets/scss",
    "build:css": "npm run scss",
    "watch:css": "onchange \"src/assets/scss/*.scss\" -- npm run build:css",
    "start:css": "react-scripts start | npm run watch:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "standard --fix | snazzy"
  },
  "devDependencies": {
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
